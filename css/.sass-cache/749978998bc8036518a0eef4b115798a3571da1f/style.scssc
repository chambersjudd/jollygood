table { border-collapse: collapse; border-spacing: 0; }
td { vertical-align: top; }


/* ==|== primary styles =====================================================
   Author:
   ========================================================================== */


body {
	background:#f9f9f9;
	color: $light;
	font-size: 0.75em; /* 12 / 16 */
	line-height: 1.5em; /* 18 / 12 */
	font-family: "Lucida Grande","Lucida Sans Unicode", helvetica, verdana, arial, sans-serif;
}

a {
	color: $light;
	text-decoration: none;
	border-bottom: $dotted;
	@include transition();
	&:hover {
		border-bottom-color:transparent;
	}
}

div.container {
	display: table;
	width: 100%;
	height: 100%;
	table-layout: fixed;

	div#inner {

		display: table-cell;
		vertical-align: middle;

		div#content {
			background: #fff;
			border: 1px solid #eee;
			padding: 2em;
			width: 580px;
			margin:0 auto 0;
			position: relative;
			text-align: center;
	
			h1 {
				text-align: center;
				margin: 0.5em 0 17px;
				padding: 0 1em;
				line-height: 1.6em;
				font-size: 1.5em;
				color: $text;
				a {
					color: $text;
				}
			}
			
			h2 {
				color: $text;
				font-family:helvetica, arial, sans-serif;
				text-transform: uppercase;
				letter-spacing: 0.1em;
				font-size: 2em;
			}

			p.info {
				text-align: center;
				margin: 0 0 2.4em;
				line-height: 1.6em;
			}


		}
	}
}

p#status {
	margin: 1em 0 0;
	color: #001B59;
	a {
		margin: 2em 0 ;
		display:inline;
		color: #001B59;
		border-bottom: 1px solid #001B59;
		&:hover {
			border-bottom-color: transparent;
		}
	}
}



div#input {
	margin: 0 0 4em;
	color: $text;
	input {
		width: 120px;
		font-family: "Lucida Grande","Lucida Sans Unicode", helvetica, verdana, arial, sans-serif;
	}
	button {
		color: #fff;
		font-family: helvetica, verdana, arial, sans-serif;
		text-align: center;
		text-transform: uppercase;
		font-size: 1em;
		letter-spacing: .1em;
		padding: 0.4em 1em;
		margin: 1em 0 0;
		text-decoration: none;
		background: $text;
		border: none;
	}
}


p.contribute {
	display: none;
	margin: 1em 0 3.5em ;
	text-align: center;

}

footer {


	p.please-dont-sue {
		margin-top: 3.5em;
		color: #999;
		a {
			color: #999;
			border: none;
		}
	}



	nav#credits {
		width: 358px;
		margin: 0 auto;
		a,
		div,
		iframe {
			float: left;
			display: inline;
			margin: 0 0.5em;
		}
		a.made-by {
			margin-right: 1em;
		}
		div.fb-like {
			margin-top: 0px;
		}
	}
}

