:@template")3$yellow:#FBEE84;
$text:#1b1b1b;
$button:#ededed;
$light:#9D9D9C;
$border:1px solid $text;
$dotted:1px dotted #bbb;
$background:#D7DBE3;

$twitter:#0091EE;

$font-size:0.85em; /* 12/16*/
$line: 1.6666666667em; /* 20/16 */

$browser-border:#9D9D9C;
$dotted-border:1px dotted #ccc;

/*controls logo spacing*/
$cellHeight: 40px;

@mixin button-text() {
	color: $text;
	font-weight: normal;
	
	letter-spacing: .1em;
/*	text-transform: uppercase;*/
	text-decoration: none;
/*	font-size: $font-size; */
}

@mixin transition($duration:0.25s) {
	 -webkit-transition: all  $duration ease-out; 
	     -moz-transition: all $duration ease-out; 
	      -ms-transition: all $duration ease-out; 
	       -o-transition: all $duration ease-out; 
	          transition: all $duration ease-out;
}

@mixin gradient($from:#eee, $to:#ddd) {
	background-color:$from;
	background-image: -webkit-gradient(linear, left top, left bottom, from($from), to($to)); 
	background-image: -webkit-linear-gradient(top, $from, $to); 
	background-image:    -moz-linear-gradient(top, $from, $to); 
	background-image:     -ms-linear-gradient(top, $from, $to); 
	background-image:      -o-linear-gradient(top, $from, $to); 
	background-image:         linear-gradient(top, $from, $to);
	
}


/**
 * Rounded corners
 */
@mixin corners($radius:7px, $type:all) {
	
	@if ($type == tl or $type == tr or $type == bl or $type == br) {
		
		@if $type == tl {
	
    		-moz-border-radius-topleft: 	$radius;
				border-top-left-radius: 	$radius;

		} @else if $type == tr {
	
    		-moz-border-radius-topright: 	$radius;
				border-top-right-radius: 	$radius;

		} @else if $type == bl {
	
    		-moz-border-radius-bottomleft: 	$radius;
				border-bottom-left-radius: 	$radius;
		
		} @else if $type == br {

    		-moz-border-radius-bottomright: $radius;
				border-bottom-right-radius: $radius;

		}

	} @else {
		
	    -moz-border-radius: 	$radius; 
		-webkit-border-radius: 	$radius; 
		 border-radius: 		$radius;
	}
}

